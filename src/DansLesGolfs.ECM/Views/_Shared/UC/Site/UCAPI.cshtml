@using DansLesGolfs.Base
@using DansLesGolfs.BLL
@using System.Globalization
@{
}
<div class="row">
    <div class="col-md-12">
        
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">
                    Chronogolf API Config
                </h3>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="form-group">
                        <div class="col-md-3 control-label">
                            <label for="ChronogolfClubId">Chronogolf Club Id</label>
                        </div>
                        <div class="col-md-2">
                            @Html.TextBox("ChronogolfClubId", DataManager.ToString((object)ViewBag.ChronogolfClubId), new { min = 0, type = "number", @class = "form-control" })
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
