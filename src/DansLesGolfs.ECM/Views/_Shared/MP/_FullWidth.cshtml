@{
}
<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!-->
<html lang="@CultureHelper.GetCurrentNeutralCulture()" dir="@(CultureHelper.IsRightToLeft() ? "rtl" : "ltr")">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    <title>@Html.MetaTitle(ViewBag.Title != null ? (string)ViewBag.Title : "")</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <meta http-equiv="Content-type" content="text/html; chartset=utf-8" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Assets/Libraries/jquery-ui/themes/custom/jquery-ui.min.css")" />
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Assets/Libraries/jquery-ui/themes/custom/jquery-ui.theme.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Assets/Libraries/bootstrap/css/bootstrap.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Assets/Libraries/bootstrap/css/bootstrap-theme.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Assets/Libraries/uniform/css/uniform.default.min.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Assets/Libraries/uniform/themes/danslesgolfs/css/uniform.danslesgolfs.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Assets/Libraries/fancybox/source/jquery.fancybox.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Assets/Libraries/dlg-cart-popup/dlg.cart-popup.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Assets/Front/css/style.css")" />
    <link rel="stylesheet" href="@Url.Content("~/Assets/Front/css/style_hockie.css")" />
    @RenderSection("head", false)
</head>
<body class="full-width @ViewBag.BodyClasses">
    <div id="fb-root"></div>
    @Html.Partial("~/Views/_Shared/UC/TopNav.cshtml")
    <div id="container">
        <header id="header">
            <h1 id="logo"><a href="@Url.Content("~/")">Dans Les Golfs</a></h1>

        </header>
        <div id="content">
            <div id="content-body">
                @RenderBody()
            </div>
        </div>
    </div>
    <a id="scroll-up-button" href="javascript:void(0)">Up</a>
    @*@Html.Raw(JSNLog.JavascriptLogging.Configure())*@
    <script>
        window.FBAppId = '@ViewBag.FBAppId';
        window.googleAPIKey = '@ViewBag.GoogleAPIKey';
        window.defaultLatitude = '@ViewBag.DefaultLatitude';
        window.defaultLongitude = '@ViewBag.DefaultLongitude';
        window.locale = 'fr_FR';
    </script>
    <script src="@Url.Content("~/Assets/Libraries/jquery-1.10.2.min.js")"></script>
    <script src="@Url.Content("~/Assets/Front/plugins/jquery-ui/js/jquery-ui-1.10.4.custom.min.js")"></script>
    <script src="@Url.Content("~/Assets/Libraries/uniform/jquery.uniform.min.js")"></script>
    <script src="@Url.Content("~/Assets/Libraries/fancybox/source/jquery.fancybox.js")"></script>
    @if (((int)ViewBag.CurrentCultureId) > 1)
    {
        <script src="@Url.Content("~/Assets/Libraries/dlg-cart-popup/i18n/" + (string)ViewBag.CurrentCulture + ".js")"></script>
    }
    <script src="@Url.Content("~/Assets/Libraries/dlg-cart-popup/dlg.cart-popup.js")"></script>
    <script src="@Url.Content("~/Assets/Front/plugins/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Front/scripts/script.js")"></script>
    @RenderSection("foot")
</body>
</html>