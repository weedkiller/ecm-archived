@{

}
<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en" class="no-js">
<!--<![endif]-->
<!-- BEGIN HEAD -->
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <meta name="MobileOptimized" content="320">
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="@Url.Content("~/Assets/Admin/plugins/font-awesome/css/font-awesome.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Admin/plugins/bootstrap/css/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Admin/plugins/uniform/css/uniform.default.css")" rel="stylesheet" type="text/css" /><!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN PAGE LEVEL PLUGIN STYLES -->
    <link href="@Url.Content("~/Assets/Admin/plugins/gritter/css/jquery.gritter.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Admin/plugins/bootstrap-daterangepicker/daterangepicker-bs3.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Admin/plugins/fullcalendar/fullcalendar/fullcalendar.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Admin/plugins/jqvmap/jqvmap/jqvmap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Admin/plugins/jquery-easy-pie-chart/jquery.easy-pie-chart.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Admin/plugins/data-tables/DT_bootstrap.css")" rel="stylesheet" type="text/css" />
    <!-- END PAGE LEVEL PLUGIN STYLES -->
    <!-- BEGIN THEME STYLES -->
    <link href="@Url.Content("~/Assets/Admin/css/style-metronic.css")" rel="stylesheet" )" type="text/css" />
    <link href="@Url.Content("~/Assets/Admin/css/style.css")" rel="stylesheet" )" type="text/css" />
    <link href="@Url.Content("~/Assets/Admin/css/style-responsive.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Admin/css/plugins.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Admin/css/pages/tasks.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Assets/Admin/css/themes/default.css")" rel="stylesheet" type="text/css" id="style_color" />
    <link href="@Url.Content("~/Assets/Admin/css/custom.css")" rel="stylesheet" type="text/css" />
    @RenderSection("head")
    ><!-- END THEME STYLES -->
    <link rel="shortcut icon" href="favicon.ico" />
</head>
<!-- END HEAD -->
<!-- BEGIN BODY -->
<body class="page-header-fixed">
    <!-- BEGIN HEADER -->
    <div class="header navbar navbar-inverse navbar-fixed-top">
        <!-- BEGIN TOP NAVIGATION BAR -->
        <div class="header-inner">
            <!-- BEGIN LOGO -->
            <a class="navbar-brand" href="@Url.Content("~/")">
                <img src="@Url.Content("~/Assets/Admin/img/logo.png")" alt="logo" class="img-responsive" />

            </a>
            <!-- END LOGO -->
            <!-- BEGIN RESPONSIVE MENU TOGGLER -->
            <a href="javascript:;" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <img src="@Url.Content("~/Assets/Admin/img/menu-toggler.png")" alt="" />

            </a>
            <!-- END RESPONSIVE MENU TOGGLER -->
            <!-- BEGIN TOP NAVIGATION MENU -->
            <ul class="nav navbar-nav pull-right">
                <!-- BEGIN USER LOGIN DROPDOWN -->
                <li class="dropdown user">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" data-close-others="true">
                        <img alt="" src="@Url.Content("~/Assets/Admin/img/avatar1_small.jpg")" />
                        >
                        <span class="username">
                            Bob Nilson
                        </span>
                        <i class="fa fa-angle-down"></i>
                    </a>
                    <ul class="dropdown-menu">
                        <li>
                            <a href="@Url.Content("~/MyProfile")"><i class="fa fa-user"></i> My Profile</a>
                        </li>
                        <li class="divider">
                        </li>
                        <li>
                            <a href="javascript:;" id="trigger_fullscreen"><i class="fa fa-move"></i> Full Screen</a>
                        </li>
                        <li>
                            <a href="@Url.Content("~/Logout")"><i class="fa fa-key"></i> Log Out</a>
                        </li>
                    </ul>
                </li>
                <!-- END USER LOGIN DROPDOWN -->
            </ul>
            <!-- END TOP NAVIGATION MENU -->
        </div>
        <!-- END TOP NAVIGATION BAR -->
    </div>
    <!-- END HEADER -->
    <div class="clearfix">
    </div>
    <!-- BEGIN CONTAINER -->
    <div class="page-container">
        <!-- BEGIN SIDEBAR -->
        <div class="page-sidebar-wrapper">
            <div class="page-sidebar navbar-collapse collapse">
                <!-- BEGIN SIDEBAR MENU -->
                @Html.Partial("~/Views/_Shared/UC/Admin/MenuList.cshtml")
                <!-- END SIDEBAR MENU -->
            </div>
        </div>
        <!-- END SIDEBAR -->
        <!-- BEGIN CONTENT -->
        <div class="page-content-wrapper">
            <div class="page-content">
                <div class="row">
                    <div class="col-md-12">
                        <!-- BEGIN PAGE TITLE & BREADCRUMB-->
                        <h3 class="page-title">
                            @ViewBag.TitleName <small>@ViewBag.Description</small>
                        </h3>
                        <ul class="page-breadcrumb breadcrumb">
                            <li>
                                <i class="fa fa-home"></i>
                                <a href="@Url.Content("~/")">
                                    Dashboard
                                </a>
                                <i class="fa fa-angle-right"></i>
                            </li>
                            @if (ViewBag.Breadcrumbs != null)
                            {
                                foreach (var entry in (Dictionary<string, string>)ViewBag.Breadcrumbs)
                                {
                                    <li>
                                        <a href="@Url.Content(entry.Value)">
                                            @entry.Key
                                        </a>
                                        <i class="fa fa-angle-right"></i>
                                    </li>
                                }
                            }
                        </ul>
                        <!-- END PAGE TITLE & BREADCRUMB-->
                    </div>
                </div>
                @if (ViewBag.SuccessMessage != null)
                {
                    <div class="alert alert-success">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                        @ViewBag.SuccessMessage
                    </div>
                }
                @if (ViewBag.ErrorMessage != null)
                {
                    <div class="alert alert-danger">
                        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                        @ViewBag.ErrorMessage
                    </div>
                }
                @RenderBody()
            </div>
        </div>
        <!-- END CONTENT -->
    </div>
    <!-- END CONTAINER -->
    <!-- BEGIN FOOTER -->
    <div class="footer">
        <div class="footer-inner">
            2013 &copy; Metronic by keenthemes.
        </div>
        <div class="footer-tools">
            <span class="go-top">
                <i class="fa fa-angle-up"></i>
            </span>
        </div>
    </div>
    <!-- END FOOTER -->
    <!-- BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->
    <!-- BEGIN CORE PLUGINS -->
    <!--[if lt IE 9]>
    <script src="@Url.Content("~/Assets/Admin/plugins/respond.min.js")"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/excanvas.min.js")")"></script>
    <![endif]-->
    <script src="@Url.Content("~/Assets/Admin/plugins/jquery-1.10.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/jquery-migrate-1.2.1.min.js")" type="text/javascript"></script>
    <!-- IMPORTANT! Load jquery-ui-1.10.3.custom.min.js before bootstrap.min.js to fix bootstrap tooltip conflict with jquery ui tooltip -->
    <script src="@Url.Content("~/Assets/Admin/plugins/jquery-ui/jquery-ui-1.10.3.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/bootstrap/js/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/bootstrap-hover-dropdown/twitter-bootstrap-hover-dropdown.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/jquery-slimscroll/jquery.slimscroll.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/jquery.blockui.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/jquery.cokie.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/uniform/jquery.uniform.min.js")" type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script src="@Url.Content("~/Assets/Admin/plugins/jqvmap/jqvmap/jquery.vmap.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/jqvmap/jqvmap/maps/jquery.vmap.russia.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/jqvmap/jqvmap/maps/jquery.vmap.world.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/jqvmap/jqvmap/maps/jquery.vmap.europe.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/jqvmap/jqvmap/maps/jquery.vmap.germany.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/jqvmap/jqvmap/maps/jquery.vmap.usa.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/jqvmap/jqvmap/data/jquery.vmap.sampledata.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/flot/jquery.flot.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/flot/jquery.flot.resize.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/jquery.pulsate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/bootstrap-daterangepicker/moment.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/bootstrap-daterangepicker/daterangepicker.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/gritter/js/jquery.gritter.js")" type="text/javascript"></script>
    <!-- IMPORTANT! fullcalendar depends on jquery-ui-1.10.3.custom.min.js for drag & drop support -->
    <script src="@Url.Content("~/Assets/Admin/plugins/fullcalendar/fullcalendar/fullcalendar.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/jquery-easy-pie-chart/jquery.easy-pie-chart.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/jquery.sparkline.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/jquery-validation/dist/jquery.validate.min.js")" type="text/javascript"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="@Url.Content("~/Assets/Admin/scripts/app.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/scripts/index.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/scripts/tasks.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/data-tables/jquery.dataTables.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Assets/Admin/plugins/data-tables/DT_bootstrap.js")" type="text/javascript"></script>
    <!-- END PAGE LEVEL SCRIPTS -->

    <script>
        @if (ViewBag.ClassName != null && ViewBag.ClassName != "")
        {
            <text>menuClassName = "menu-@ViewBag.ClassName";</text>
        }
        jQuery(document).ready(function () {
            App.init(); // initlayout and core plugins
            Index.init();
            Index.initJQVMAP(); // init index page's custom scripts
            Index.initCalendar(); // init index page's custom scripts
            Index.initCharts(); // init index page's custom scripts
            Index.initChat();
            Index.initMiniCharts();
            Index.initDashboardDaterange();
            Index.initIntro();
            Tasks.initDashboardWidget();
        });
    </script>
    <script src="@Url.Content("~/Assets/Admin/scripts/admin.js")" type="text/javascript"></script>
    @RenderSection("foot")
    <!-- END JAVASCRIPTS -->
</body>
<!-- END BODY -->
</html>